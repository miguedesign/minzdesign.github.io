@font-face {
	font-family: 'minz';
	src:url('../icons/minz.eot?dkteel');
	src:url('../icons/minz.eot?dkteel#iefix') format('embedded-opentype'),
		url('../icons/minz.ttf?dkteel') format('truetype'),
		url('../icons/minz.woff?dkteel') format('woff'),
		url('../icons/minz.svg?dkteel#minz') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'minz' !important;
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

